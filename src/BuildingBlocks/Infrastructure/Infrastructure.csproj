<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="CrossCutting\Services\Email\**" />
    <Compile Remove="CrossCutting\Services\Logging\**" />
    <Compile Remove="CrossCutting\Services\Sms\**" />
    <EmbeddedResource Remove="CrossCutting\Services\Email\**" />
    <EmbeddedResource Remove="CrossCutting\Services\Logging\**" />
    <EmbeddedResource Remove="CrossCutting\Services\Sms\**" />
    <None Remove="CrossCutting\Services\Email\**" />
    <None Remove="CrossCutting\Services\Logging\**" />
    <None Remove="CrossCutting\Services\Sms\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Application\Resources\ApplicationMessages.Designer.cs">
      <DependentUpon>ApplicationMessages.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="Application\Resources\ApplicationMetadata.Designer.cs">
      <DependentUpon>ApplicationMetadata.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Application\Resources\ApplicationMessages.resx">
      <LastGenOutput>ApplicationMessages.Designer.cs</LastGenOutput>
      <Generator>PublicResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Application\Resources\ApplicationMetadata.resx">
      <LastGenOutput>ApplicationMetadata.Designer.cs</LastGenOutput>
      <Generator>PublicResXFileCodeGenerator</Generator>
    </EmbeddedResource>
  </ItemGroup>

</Project>
